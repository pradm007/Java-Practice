import java.util.*;

public class Rotatearray {

	public static void main(String[] args) {

		int[] array = {1,2,3,4,5};
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("Enter the number of rotations: ");
		int k = scanner.nextInt();
		
		
	}
	
	void swap(int a, int b) {
		int temp = a;
		a = b;
		b = temp;
	}

}
